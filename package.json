{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "New template for building nuxt projects",
  "author": "Mark Gomez",
  "keywords": ["nuxt-quill","vue quill", "nuxt quill-better-table"],
  "bugs": "https://github.com/gomezmark/nuxt-quill-template/issues",
  "files": ["*"],
  "repository": {
    "url": "https://github.com/gomezmark/nuxt-quill-template",
    "type": "git"
  },
  "private": true,
  "license": "MIT",
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "8002"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.0.0",
    "url-loader": "^3.0.0",
    "v-calendar": "^0.9.7",
    "vue-moment": "^4.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "file-loader": "^5.0.2",
    "nodemon": "^1.18.9",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10"
  }
}
